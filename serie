// serie.cpp : main project file.

#include "stdafx.h"
#include "stdio.h"
#include "stdlib.h"
#include "conio.h"


 

void main()
{float A,n,d;
printf("ingrese numero tope para la serie : ");
scanf("%f",&n);
A=0;
while(n>=d)
for(d=0;d<=n;d=d+1)
{A= A +((n-d)/(d+1));
}
printf(" a es igual: %f",A);
printf("\n");
printf("\n");
system("pause");

}
