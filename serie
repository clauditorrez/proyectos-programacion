// serie.cpp : main project file.

#include "stdafx.h"
#include "stdio.h"
#include "stdlib.h"
#include "conio.h"


 

void main()

{int n,e,d;
  float A;
printf("ingrese numero tope para la serie : ");
scanf("%d",&n);
A=0;
d=1;
e=0;
while(n>=d)
{A= (((n-e)/d)+A);
e=e+1;
d=d+1;
}
printf(" a es igual: %f",A);
printf("\n");
printf("\n");
system("pause");

}
